<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="88dp"
    android:paddingLeft="16dp"
    android:descendantFocusability="blocksDescendants"
    >

    <ImageView
        android:paddingTop="16dp"
        android:paddingRight="32dp"
        android:id="@+id/ivRezer"
        android:src="@drawable/baseline_event_black_24dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_toRightOf="@+id/ivRezer"
        android:id="@+id/nazivProstorije"
        android:text="Naziv Prostorije"
        android:textSize="16dp"
        android:gravity="bottom"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:clickable="false"/>

    <TextView
        android:gravity="bottom"
        android:layout_toRightOf="@+id/ivRezer"
        android:id="@+id/terminRezervacije"
        android:layout_below="@+id/nazivProstorije"
        android:text="Termin"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:clickable="false"/>
    <CheckBox
        android:layout_toRightOf="@+id/ivRezer"
        android:id="@+id/cbOdobrena"
        android:layout_below="@id/terminRezervacije"
        android:text="Odobrena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:focusable="false"
        android:focusableInTouchMode="false"
        android:clickable="false"
        />


</RelativeLayout>