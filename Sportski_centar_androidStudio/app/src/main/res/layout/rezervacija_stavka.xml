<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="blocksDescendants"
    android:padding="15dp">

    <TextView
        android:id="@+id/nazivProstorije"
        android:text="Naziv Prostorije"
        android:textSize="15dp"
        android:textStyle="bold"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:clickable="false"/>

    <TextView
        android:id="@+id/terminRezervacije"
        android:layout_below="@+id/nazivProstorije"
        android:paddingTop="7dp"
        android:text="Termin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:clickable="false"/>
    <CheckBox
        android:id="@+id/cbOdobrena"
        android:layout_below="@id/terminRezervacije"
        android:text="Odobrena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:focusable="false"
        android:focusableInTouchMode="false"
        android:clickable="false"
        />


</RelativeLayout>